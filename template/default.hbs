        <script>
            SmartAjax_load('/js/smartajax/', function(){
            SmartAjax.setOptions({
                history:true,
                cache:true,
                analytics:true,
                 containers:
                    [
                        {selector: '.tabs-content:first'},
                        {selector: '.tabs:first'},
                        {selector: '.tabs-scripts'}
                    ],
                before: function() {
                   $( ".tabs-content:first .wrapper:first" ).fadeOut( 700 , SmartAjax.proceed);       
                },
                success: function() {
                   SmartAjax.proceed();
                },
                done: function() {
                   $( ".tabs-content:first .wrapper:first" ).fadeIn( 700);
                }
            });
            SmartAjax.bind('a.ajax');
        });
        </script>
